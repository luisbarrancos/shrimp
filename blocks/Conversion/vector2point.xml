<shrimp type="function" name="Vector2Point" description="Vector to point conversion" author="rconstruct">
    <input name="in" description="Input value" type="vector" default="0"/>
    <output name="out" description="Output value" type="point"/>
	<rsl_code>
	    $(out) = point( comp( $(in), 0), comp( $(in), 1), comp( $(in), 2) );
	</rsl_code>
</shrimp>
