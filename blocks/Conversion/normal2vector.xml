<shrimp type="function" name="Normal2Vector" description="Normal to vector conversion" author="rconstruct">
    <input name="in" description="Input value" type="normal" default="0"/>
    <output name="out" description="Output value" type="vector"/>
	<rsl_code>
	    $(out) = vector( comp( $(in), 0), comp( $(in), 1), comp( $(in), 2) );
	</rsl_code>
</shrimp>
